#!/usr/bin/env ruby
$: << File.expand_path("../../lib", __FILE__)
require 'babelish'
require 'babelish/command'

puts "DEPRECATED: use babelish command"
args = [File.basename(__FILE__)]
if ARGV.first == "help"
  args.insert(0, ARGV.first)
  ARGV.delete(ARGV.first)
end
args += ARGV
Babelish::Command.start(args)
